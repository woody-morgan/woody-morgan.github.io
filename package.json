{
  "name": "woody-morgan.github.io",
  "version": "2.0.0",
  "homepage": "https://woody-morgan.github.io",
  "scripts": {
    "start": "cross-env SOCKET=true node ./scripts/next-remote-watch.js ./data",
    "dev": "next dev",
    "build": "next build && node ./scripts/generate-sitemap",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "deploy": "next build && next export -o docs && touch docs/CNAME && touch docs/.nojekyll && gh-pages -t true -d docs",
    "compose": "node ./scripts/compose.js"
  },
  "dependencies": {
    "@channel.io/channel-web-sdk-loader": "^1.1.6",
    "@fontsource/inter": "4.5.2",
    "@mailchimp/mailchimp_marketing": "^3.0.58",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.4.0",
    "esbuild": "^0.13.13",
    "framer-motion": "^6.2.8",
    "gh-pages": "^5.0.0",
    "github-slugger": "^1.3.0",
    "gray-matter": "^4.0.2",
    "image-size": "1.0.0",
    "js-yaml": "^4.1.0",
    "mdx-bundler": "^8.0.0",
    "next": "12.0.9",
    "next-themes": "^0.0.14",
    "notion-client": "^4.19.8",
    "postcss": "^8.4.5",
    "preact": "^10.6.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reading-time": "1.3.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-citation": "^0.2.0",
    "rehype-katex": "^6.0.2",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.1.3",
    "rehype-slug": "^5.0.0",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "sharp": "^0.28.3",
    "smoothscroll-polyfill": "^0.4.4",
    "tailwindcss": "^3.0.18",
    "unist-util-visit": "^4.0.0",
    "vfile": "5.0.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.0.9",
    "@svgr/webpack": "^6.1.2",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.4.0",
    "@types/react": "^17.0.14",
    "@types/tailwindcss": "^2.2.0",
    "@types/unist": "2.0.6",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "eslint": "^7.29.0",
    "eslint-config-next": "12.0.9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "file-loader": "^6.0.0",
    "globby": "11.0.3",
    "husky": "^6.0.0",
    "inquirer": "^8.1.1",
    "lint-staged": "^11.0.0",
    "next-remote-watch": "^1.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "socket.io": "^4.6.2",
    "socket.io-client": "^4.4.0",
    "typescript": "^4.3.5"
  },
  "packageManager": "pnpm@8.6.2",
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
